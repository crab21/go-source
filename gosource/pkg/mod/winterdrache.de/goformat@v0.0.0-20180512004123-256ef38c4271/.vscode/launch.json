    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Debug goformat",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/src/goformat/goformat/gofmt.go",
            "args": [
                "-fragment",
                "--style=${workspaceRoot}/src/goformat/goformat/testdata/style1",
                "${workspaceRoot}/src/goformat/goformat/testdata/selectswitch.input"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "showLog": false,
            "output": "${workspaceRoot}/bin/debug-temp"
        }
   
    ]
}